<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device

    -width, initial-scale=1.0">
    <title>{{ siteName }}</title>
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.8.1/css/all.css">
    <link rel="stylesheet" type="text/css" href="//cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick.css"/>
    <link rel="stylesheet" href="{{ siteUrl }}assets/index.css">
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAdUOKXNaY_aFRCYWD20YhD6XVhbhrqGzw&callback=initMap&libraries=&v=beta&map_ids=7a0d902c2626c4f5" defer ></script>    <!-- jsdelivr -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/3.5.1/gsap.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/@barba/core"></script>
    <script src="https://polyfill.io/v3/polyfill.min.js?features=default"></script>
    <link href="https://vjs.zencdn.net/7.8.4/video-js.css" rel="stylesheet" />

</head>
<body data-barba="wrapper">

<header class="header grid-row">
    <div class="grid align-full">
        <nav class="nav">
            {% for link in header.navigation|slice(0, 3) %}
                {% set destination = link.linkDestination.one() %}
                <a href="{{ destination.url }}"
                   class="nav-link">{{ link.linkText }}</a>
            {% endfor %}
        </nav>
        <a href="{{ siteUrl }}" class="logo">
            <i class="fas fa-image"></i>
        </a>
        <nav class="nav">
            {% for link in header.navigation|slice(3, 6) %}
                {% set destination = link.linkDestination.one() %}
                <a href="{{ destination.url }}"
                   class="nav-link"><p>{{ link.linkText }}</p></a>
            {% endfor %}
        </nav>
    </div>
</header>
<main data-barba="container" data-barba-namespace="home">
    <article>
        {% block content %}
            {# Page template inserted here #}
        {% endblock %}
    </article>
</main>
<footer class="footer grid-row">
    <div class="grid align-wide contact">
        <div class="block">
            {% set destination = footer.channelLinkDestination.one() %}
            <h3><a href="{{ destination.url }}" class="nav-link button">{{ footer.channelLinkText }}</a></h3>
            {{ footer.address }}
        </div>
        <nav class="nav">
            {% set destination = services.channelLinkDestination.one() %}
            <h3><a href="{{ destination.url }}" class="nav-link button">{{ services.channelLinkText }}</a></h3>
            <ul>
                {% for link in services.navigation %}
                    {% set destination = link.linkDestination.one() %}
                    <li><a href="{{ destination.url }}"
                           class="nav-link button">{{ link.linkText }}</a></li>
                {% endfor %}
            </ul>
        </nav>
        <nav class="nav">
            {% set destination = caseStudies.channelLinkDestination.one() %}
            <h3><a href="{{ destination.url }}" class="nav-link button">{{ caseStudies.channelLinkText }}</a></h3>
            <ul>
                {% for link in caseStudies.navigation %}
                    {% set destination = link.linkDestination.one() %}
                    <li><a href="{{ destination.url }}"
                           class="nav-link button">{{ link.linkText }}</a></li>
                {% endfor %}
            </ul>
        </nav>
        <div class="social">
            <a href=""><i class="fab fa-instagram"></i></a>
            <a href=""><i class="fab fa-twitter"></i></a>
            <a href=""><i class="fab fa-linkedin"></i></a>
            <a href=""><i class="fab fa-facebook"></i></a>
        </div>
    </div>
    <div class="grid align-wide strapline">
        <div class="block">
            {{ footer.richText }}
        </div>
        <nav class="nav">
            {% for link in footer.navigation %}
                {% set destination = link.linkDestination.one() %}
                <a href="{{ destination.url }}"
                   class="nav-link button">{{ link.linkText }}</a>
            {% endfor %}
        </nav>
    </div>
</footer>
<script src="http://code.jquery.com/jquery-3.5.1.min.js" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0="
        crossorigin="anonymous"></script>
<script type="text/javascript" src="//cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick.min.js"></script>
<script src="{{ siteUrl }}assets/script.js"></script>


</body>
</html>